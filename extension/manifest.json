{
    "name": "YT-Downloader",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Esto aparecera como descripcion de la extension",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "downloads"
    ],
    "short_name": "YT-Downloader",
    "author": "Rogelio",
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "YT-Downloader"
    },
    "content_security_policy": "script-src 'self' https://cdn.socket.io/3.1.3/socket.io.min.js; object-src 'self'"
}